#
# Rediagonalized alanine
#
firststep 0
numsteps              20
outputfreq            25
debug 0

finxyzposfile LeapfrogTruncatedShadowTest.xyz
#

# Constraints
angularMomentum 0
comMotion 0
exclude scaled1-4
#shake true

seed 1234
screen false
posfile alanine.pdb
psffile alanine.psf
parfile alanine.par
temperature 300

boundaryConditions vacuum

cellManager Cubic
cellsize 5

Integrator {
  level 0 LeapfrogTruncatedShadow {
    
    timestep 0.5
    
    force Improper
    force Dihedral
    force Bond
    force Angle
    force LennardJones Coulomb
        -algorithm NonbondedSimpleFull

  }
}