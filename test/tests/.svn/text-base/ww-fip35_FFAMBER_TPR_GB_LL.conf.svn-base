firststep   0
numsteps    20
outputfreq  20

# Constraints
angularMomentum 0
comMotion 0
exclude scaled1-4

seed 1234

# Inputs
gromacstprfile	data/ww-fip35-ffamber-gb.tpr
temperature 	300

# Outputs
dcdfile 	output/ww-fip35_FFAMBER_TPR_GB_LL.dcd
XYZForceFile	output/ww-fip35_FFAMBER_TPR_GB_LL.forces
finXYZPosFile   output/ww-fip35_FFAMBER_TPR_GB_LL.pos
finXYZVelFile   output/ww-fip35_FFAMBER_TPR_GB_LL.vel
allenergiesfile output/ww-fip35_FFAMBER_TPR_GB_LL.energy

boundaryConditions vacuum

cellManager Cubic
cellsize 5

doGBSAObc 2

Integrator {

  level 0 LangevinLeapfrog {

    timestep 1
    temperature 300
    gamma 91
    seed 1234

    force Bond
    force Angle
    force RBDihedral
    force Dihedral

    force LennardJones
            -algorithm NonbondedSimpleFull

    force Coulomb
            -algorithm NonbondedSimpleFull

    force GBBornBurialTerm
            -algorithm NonbondedSimpleFull

    force GBBornRadii
           -algorithm NonbondedSimpleFull

    force GBForce
           -algorithm NonbondedSimpleFull
           -soluteDielec 1.0
           -solventDielec 78.3

    force GBACEForce
           -algorithm NonbondedSimpleFull

 }
}

